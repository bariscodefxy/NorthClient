                    <!-- Content wrapper -->

                    <div class="content-wrapper">
                        <!-- Content -->
                        <div class="container-xxl flex-grow-1 container-p-y">

                            <div class="main-panel">
                                <div class="content">
                                    <div class="page-inner">
                                        <div class="mt-2 mb-4">
                                            <% if (req.query.err) { %>
                                            <% if (req.query.err !== "none") { %>
                                            <h5 class="text-white op-7 mb-4">
                                                <% if (req.query.err == "MISSINGID") { %>
                                                <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>You have not provided the 
                                                            user Email.</strong></span>
                                                    <button type="button" class="close" data-dismiss="alert"
                                                        aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                                <% } else if (req.query.err == "INVALIDID" || req.query.err == "MISSINGID") { %>
                                                <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>You have provided a user Email
                                                            that does not exist, or has not signed up on the
                                                            dashboard.</strong></span>
                                                    <button type="button" class="close" data-dismiss="alert"
                                                        aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                                <% } 
                    else if (req.query.err == "CREATECOUPONINVALIDCHARACTERS") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                One of the characters you entered in coupon creation was invalid, please
                                                try again.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "REVOKECOUPONCANNOTFINDCODE") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                One of the characters you entered in coupon revoke was invalid, please
                                                try again.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "CREATECOUPONEMPTY") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                While creating your coupon you did not specify to give any resources or
                                                coins.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "CREATECOUPONLESSTHANONE") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                While creating your coupon you tried to give less then one in RAM, DISK,
                                                CPU and/or coins.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "MISSINGVARIABLES") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                You have not provided either ram, disk, cpu, and/or servers.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "REMOVEACCOUNTMISSINGID") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                You have not specified an id to remove from the panel
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "MISSINGCOINS") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                You have not provided the coin amount.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "RAMSIZE" || req.query.err == "DISKSIZE" || req.query.err == "CPUSIZE" || req.query.err == "SERVERSIZE") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                The RAM, disk, CPU and servers number cannot be less than 0 nor above
                                                999999999999999.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "INVALIDCOINNUMBER") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                The coin amount must be a number.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else if (req.query.err == "COINSIZE") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                The coin amount canno tbe less than 0 and must be above 999999999999999.
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>

                                                <% } else if (req.query.err == "NOIP") { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                There is no IP saved for that user.
                                                <% } else if(req.query.err == "NONE" && req.query.ip !== undefined) { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
                                                IP: <%= req.query.ip %>
                                                <button type="button" class="close" data-dismiss="alert"
                                                aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                                <% } else { %>
                                                    <div class="alert alert-warning alert-dismissible fade show"
                                                    role="alert">
                                                    <span class="alert-icon"><i class="ni ni-settings"></i></span>
                                                    <span class="alert-text"><strong>
